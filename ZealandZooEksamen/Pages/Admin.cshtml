@page
@model ZealandZooEksamen.Pages.AdminModel
@{
}



<h2>Tilmeldte til Nyhedsbrev</h2>

<table>

    <thead>
    <th>Id</th>
    <th>Navn</th>
    <th>Telefon nr</th>
    <th>Deltager</th>
    </thead>

    <tbody>
        @foreach (var person in Model.Personer)
        {
            <tr>
                <td>@person.Id</td>
                <td>@person.Name</td>
                <td>@person.PhoneNumber</td>
                <td>@person.Attending</td>
            </tr>
        }
    </tbody>
</table>
<a asp-page="Index"></a>