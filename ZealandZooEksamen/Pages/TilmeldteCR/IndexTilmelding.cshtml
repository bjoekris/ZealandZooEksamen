@page
@model ZealandZooEksamen.Pages.TilmeldteCR.IndexTilmeldingModel
@{
}

<body class="bg-light">
    <h2>Tilmeldte til dette event</h2>
    <a class="btn btn-primary" asp-page="/EventCRUD/Index">Tilbage</a>
    <h6></h6>
    <table class="table table-striped table-hover bg-light rounded-2">
        <thead>
            <tr>
                <th>Id</th>
                <th>Navn</th>
                <th>Telefon</th>
            </tr>
        </thead>
        <tbody>
       
            @foreach (var t in Model.Tilmeldte)
            {
                <tr>
                    <td>@t.TilmeldingId</td>
                    <td>@t.Navn</td>
                    <td>@t.Telefon</td>
                </tr>
            }
        </tbody>
    </table>
</body>